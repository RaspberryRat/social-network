<h2>Upload Image?</h2>

<%= form_with(model: @user, url: update_profile_picture_user_path(@user), method: :patch, local: true) do |form| %>
  <div>
    <%= form.label :profile_picture %>
    <%= form.file_field :profile_picture %>
  </div>
  <div class="actions">
    <%= form.submit 'Update Profile Picture' %>
  </div>
<% end %>
